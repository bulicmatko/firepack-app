{
  "name": "firepack-app",
  "version": "0.0.0",
  "description": "Firepack Application Boilerplate",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production node ./node_modules/webpack/bin/webpack.js --optimize-minimize --progress --colors",
    "check": "NODE_ENV=development npm run lint && npm run flow && npm test",
    "cover": "NODE_ENV=development node ./node_modules/babel-istanbul/lib/cli.js cover ./node_modules/mocha/bin/_mocha ./test/**/**/**/*.js --root ./src --report html --dir ./public/coverage --include-all-sources",
    "deploy": "npm run build && node ./node_modules/firebase-tools/bin/firebase deploy",
    "flow": "NODE_ENV=development node ./node_modules/flow-bin/cli.js",
    "start": "NODE_ENV=development node ./node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --colors --open --port 8888",
    "lint": "NODE_ENV=development node ./node_modules/eslint/bin/eslint.js ./src",
    "server": "NODE_ENV=development node ./node_modules/firebase-tools/bin/firebase serve",
    "test": "NODE_ENV=development node ./node_modules/mocha/bin/mocha ./test/**/**/**/*.js --compilers js:babel-register --recursive",
    "test:watch": "NODE_ENV=development npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/bulicmatko/firepack-app.git"
  },
  "keywords": [
    "firepack",
    "firebase",
    "react",
    "redux",
    "starter",
    "boilerplate"
  ],
  "author": "Matko Bulic <bulicmatko@gmail.com>",
  "license": "MIT",
  "homepage": "https://bitbucket.org/bulicmatko/firepack-app#readme",
  "bugs": {
    "url": "https://bitbucket.org/bulicmatko/firepack-app/issues"
  },
  "dependencies": {
    "firebase": "^3.1.0",
    "lodash": "^4.13.1",
    "react": "^15.2.0",
    "react-css-modules": "^3.7.7",
    "react-dom": "^15.2.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.2",
    "react-router-redux": "^4.0.5",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "reselect": "^2.5.3"
  },
  "devDependencies": {
    "babel-core": "^6.10.4",
    "babel-eslint": "^6.1.0",
    "babel-istanbul": "^0.11.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-flow-strip-types": "^6.8.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-register": "^6.9.0",
    "bourbon": "^4.2.7",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.9.0",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.10.2",
    "eslint-plugin-jsx-a11y": "^1.5.5",
    "eslint-plugin-react": "^5.2.2",
    "firebase-tools": "^3.0.4",
    "flow-bin": "^0.28.0",
    "mocha": "^2.5.3",
    "node-sass": "^3.8.0",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "resolve-url-loader": "^1.6.0",
    "sass-loader": "^4.0.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "engines": {
    "node": ">=4.2.1 <6",
    "npm": ">=3.10.3 <4"
  }
}
